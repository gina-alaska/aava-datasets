download: curl -L "https://docs.google.com/a/alaska.edu/spreadsheets/d/1T-nP10lMuV5B20E5rDKA0rG1AES8O6AI8bAiqXrdnt4/export?exportFormat=csv#gid=311456934" > ToolikLake_TVEXPORT.csv
convert: yak geojson -f -p EPSG:4326 ToolikLake_TVEXPORT.vrt
edit: http://ubermap.gina.alaska.edu/geojson_layers/2/edit
