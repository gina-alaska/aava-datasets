download: curl -L "https://docs.google.com/a/alaska.edu/spreadsheets/d/1ZJwLMz2r4KO5uvCdRo_1hjlV8P-NdMJ1IIsbtq-gC7s/export?exportFormat=csv#gid=0" > AAVA_Datasets_Grouped.csv
convert: yak geojson -f -p EPSG:4326 -s group AAVA_Datasets_Grouped.vrt
edit: http://ubermap.gina.alaska.edu/geojson_layers/1/edit 
