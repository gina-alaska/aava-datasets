<OGRVRTDataSource>
    <OGRVRTLayer name="northslope-arcss">
        <SrcDataSource>northslope-arcss.csv</SrcDataSource>
        <GeometryType>wkbPoint</GeometryType>
        <LayerSRS>WGS84</LayerSRS>
        <GeometryField encoding="PointFromColumns" x="lon" y="lat"/>
    </OGRVRTLayer>
</OGRVRTDataSource>
